<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vcloudapi.api.member.mapper.UserMapper">
	<select id="selectOAuthUserById" parameterType="String" resultType="user">
        SELECT *
        FROM  USER_MST
        WHERE USER_ID = #{value}
    </select>

    <select id="insertOAuthUser" parameterType="user">
        INSERT INTO USER_MST()
        VALUE()
    </select>
</mapper>